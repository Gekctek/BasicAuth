{
	"webroot": "wwwroot",
	"version": "1.0.0-*",

	"dependencies": {
		"Microsoft.AspNet.Mvc": "6.0.0-beta8",
		"Microsoft.AspNet.Server.Kestrel": "1.0.0-beta8",
		"BasicAuth": "1.0.0-*"
	},

	"commands": {
		"web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls http://localhost:5000;https://localhost:5001"
	},

	"frameworks": {
		"dnx451": { },
		"dnxcore50": { }
	},

	"exclude": [
		"wwwroot",
		"node_modules",
		"bower_components"
	],
	"publishExclude": [
		"node_modules",
		"bower_components",
		"**.xproj",
		"**.user",
		"**.vspscc"
	]
}
